{
  "name": "ide-golang",
  "main": "./lib/main",
  "version": "0.0.5",
  "description": "Go language support for Atom-IDE",
  "keywords": [],
  "repository": "https://github.com/bongnv/atom-ide-golang",
  "license": "MIT",
  "scripts": {
    "lint": "tslint --project . && tslint --project ./spec",
    "test": "atom --test spec"
  },
  "engines": {
    "atom": ">= 1.19.0 <2.0.0"
  },
  "atomTestRunner": "./spec/runner",
  "atomTranspilers": [
    {
      "glob": "**/*.ts",
      "transpiler": "atom-ts-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json"
        ]
      }
    }
  ],
  "dependencies": {
    "@types/atom": "^1.26.0",
    "atom-linter": "^10.0.0",
    "atom-ts-transpiler": "^1.5.2",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/sinon": "^7.0.2",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "sinon": "^5.0.2",
    "tslint": "^5.9.1"
  },
  "consumedServices": {
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "code-format.file": {
      "versions": {
        "0.1.0": "provideFileCodeFormat"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
